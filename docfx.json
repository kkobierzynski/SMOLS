{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "**.csproj"
          ],
          "src": "./"
        }
      ],
      "dest": "Docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "Docs/api/**.yml",
          "Docs/api/index.md"
        ]
      },
      {
        "files": [
          "Docs/articles/**.md",
          "Docs/articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "Docs/images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "Docs/apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "Docs/_site/**"
        ]
      }
    ],
    "dest": "Docs/_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "statictoc"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}